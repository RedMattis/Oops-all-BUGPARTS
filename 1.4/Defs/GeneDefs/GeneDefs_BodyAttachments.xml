<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <GeneDef Name="GeneTailBase" Abstract="True">
        <biostatCpx>0</biostatCpx>
        <biostatMet>0</biostatMet>
        <randomChosen>true</randomChosen>
        <displayCategory>Bug_BodyAttachment</displayCategory>
        <displayOrderInCategory>50</displayOrderInCategory>
        <iconColor>(0.75, 0.75, 0.75)</iconColor>
        <graphicData>
            <drawLoc>Tailbone</drawLoc>
            <drawScale>1.33</drawScale>
            <drawOffsetNorth>(0, 0, 0)</drawOffsetNorth>
            <drawOffsetSouth>(0, 0, 0)</drawOffsetSouth>
            <drawOffsetEast>(0, 0, 0)</drawOffsetEast>
        </graphicData>
        <exclusionTags>
            <li>Bug_BodyAttachment</li>
            <li>Tail</li>
			<li>taur</li>
			<li>spiderlegs</li>
        </exclusionTags>
	</GeneDef>
	

