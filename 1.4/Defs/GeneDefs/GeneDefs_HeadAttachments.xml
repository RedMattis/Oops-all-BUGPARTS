	<GeneDef Name="BP_GeneAntennaBase" Abstract="True">
    <biostatCpx>0</biostatCpx>
    <endogeneCategory>Headbone</endogeneCategory>
    <displayCategory>Bug_HeadAttachment</displayCategory>
    <displayOrderInCategory>80</displayOrderInCategory>
    <iconColor>(0.75, 0.75, 0.75)</iconColor>
    <randomChosen>true</randomChosen>
    <graphicData>
      <drawNorthAfterHair>true</drawNorthAfterHair>
      <useSkinShader>true</useSkinShader>
      		<layer>PostHeadgear</layer>
			<drawLoc>HeadTop</drawLoc>
			<colorType>Skin</colorType>
    </graphicData>
    <exclusionTags>
		<li>Antennas</li>
		<li>Antenna</li>
		<li>Headbone</li>
		<li>HeadChitin</li>
    </exclusionTags>
  </GeneDef>
	
