    <iconPath>UI/Icons/Genes/BugParts_Mothfluff</iconPath>
	<graphicData>
	  <graphicPath>Things/Pawn/Humanlike/BodyAttachments/Fluff/Neckfluff</graphicPath>
	  		<drawOffsetNorth>(0.0, 0.2, -0.0)</drawOffsetNorth>
		<drawOffsetSouth>(-0.0, -0.01, -0.0)</drawOffsetSouth>
		<drawOffsetEast>(-0.0, 0.2, -0.0)</drawOffsetEast>
	</graphicData>

  	  </GeneDef>
	  
	  	<GeneDef>
		<defName>BP_Motheyes</defName>
		<label>Moth eyes</label>
		<iconPath>UI/Icons/Genes/BP_Motheyes</iconPath>
		<description>Carriers of this gene have two large eyes, that can almost see things before they happen.</description>
		<displayCategory>Bug_HeadAttachment</displayCategory>
		<displayOrderInCategory>52</displayOrderInCategory>
		<biostatMet>-1</biostatMet>
		<biostatCpx>1</biostatCpx>
		<selectionWeight>0</selectionWeight>
		<exclusionTags>
			<li>ShootingAccuracy</li>
			<li>EyeColor</li>
			<li>AG_Heads</li>
			<li>Heads</li>
			<li>Head</li>
		</exclusionTags>
		<capMods>
			<li>
				<capacity>Sight</capacity>
				<offset>0.2</offset>
			</li>
		</capMods>
			<forcedHeadTypes>
      <li>Mothhead_Average1</li>
      <li>Mothhead_Narrow1</li>
      <li>Mothhead_Gaunt</li>
	  <li>Mothhead_HeavyJaw</li>
    </forcedHeadTypes>
		<graphicData>
			<drawScale>0.28</drawScale>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/BP_BugEyes</graphicPath>
			<color>(255, 255, 255)</color>
			<visibleNorth>false</visibleNorth>
			<drawOnEyes>true</drawOnEyes>
			<layer>PostTattoo</layer>
		</graphicData>
	</GeneDef>
	
