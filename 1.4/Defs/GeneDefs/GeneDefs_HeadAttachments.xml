	<GeneDef ParentName="BP_GeneeyesBase">
    <defName>Insectiod_Mantideyes</defName>
    <label>Mantid eyes</label>
    <description>Carriers of this gene have large, powerful, compound insectoid eyes along with a mantis face.</description>
    <iconPath>Genes/Icons/BugParts_MantidEyesIcon</iconPath>
	<missingGeneRomanceChanceFactor>0.4</missingGeneRomanceChanceFactor>
	<capMods>
      <li>
        <capacity>Sight</capacity>
        <postFactor>1.2</postFactor>
      </li>
    </capMods>
	<forcedHeadTypes>
      <li>Mantidhead_Average1</li>
      <li>Mantidhead_Narrow1</li>
	  <li>Mantidhead_Narrow2</li>
      <li>Mantidhead_Gaunt</li>
	  <li>Mantidhead_HeavyJaw</li>
    </forcedHeadTypes>
	<graphicData>
	  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/MantidEyes</graphicPath>
	</graphicData>

  	  </GeneDef>
	  
	  	<GeneDef ParentName="BP_GenefloofBase">
    <defName>Moth_floof</defName>
    <label>Neck Fluff</label>
