<?xml version="1.0" ?>
<Defs>
  <GeneDef Name="BP_WingBase" Abstract="True">
    <displayCategory>Miscellaneous</displayCategory>
    <exclusionTags>
      <li>Wing</li>
      <li>Wings</li>
    </exclusionTags>
    <randomChosen>true</randomChosen>
  </GeneDef>
  <GeneDef ParentName="BP_WingBase">
    <defName>BP_MantisWings</defName>
    <label>Mantis Wings</label>
    <description>Carriers have elytra that count as slightly useable wings.</description>
    <iconPath>UI/Icons/Genes/Gene_MantisWings</iconPath>
    <displayCategory>Bug_BodyAttachment</displayCategory>
    <descriptionHyperlinks>
      <AbilityDef>BP_WingPoweredJump</AbilityDef>
    </descriptionHyperlinks>
    <biostatMet>-1</biostatMet>
    <symbolPack>
      <wholeNameSymbols>
        <li>
          <symbol>jumper</symbol>
        </li>
        <li>
          <symbol>leaper</symbol>
        </li>
        <li>
          <symbol>bouncer</symbol>
        </li>
      </wholeNameSymbols>
      <prefixSymbols>
        <li>
          <symbol>jump</symbol>
        </li>
        <li>
          <symbol>leap</symbol>
        </li>
        <li>
          <symbol>vault</symbol>
        </li>
        <li>
          <symbol>long</symbol>
        </li>
        <li>
          <symbol>fly</symbol>
        </li>
        <li>
          <symbol>hop</symbol>
        </li>
        <li>
          <symbol>wing</symbol>
        </li>
      </prefixSymbols>
    </symbolPack>
    <abilities>
      <li>BP_WingPoweredJump</li>
    </abilities>
    <renderNodeProperties Inherit="False">
      <li>
        <workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
        <parentTagDef>Body</parentTagDef>
        <texPath>Things/Pawn/Humanlike/BodyAttachments/MantisWings/MantisWings</texPath>
        <drawSize>(1.0,1.0)</drawSize>
        <colorType>Skin</colorType>
        <shaderTypeDef>CutoutComplex</shaderTypeDef>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <drawData>
          <defaultData>
            <layer>10</layer>
          </defaultData>
          <dataSouth>
            <layer>-1</layer>
          </dataSouth>
          <dataNorth>
            <layer>90</layer>
          </dataNorth>
        </drawData>
      </li>
    </renderNodeProperties>
  </GeneDef>
