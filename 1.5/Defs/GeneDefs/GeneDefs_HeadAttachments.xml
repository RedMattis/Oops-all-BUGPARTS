          <scaleOffsetByBodySize>true</scaleOffsetByBodySize>
          <defaultData>
            <layer>25</layer>
            <offset>(0.0, 0.0, 0.35)</offset>
          </defaultData>
          <dataEast>
            <layer>25</layer>
            <offset>(0.075, 0.0, 0.40)</offset>
          </dataEast>
          <dataWest>
            <layer>25</layer>
            <offset>(-0.075, 0.0, 0.40)</offset>
