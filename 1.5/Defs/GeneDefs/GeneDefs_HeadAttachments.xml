  </GeneDef>
  <GeneDef ParentName="BP_GenefloofBase">
    <defName>Moth_floof_SkinClr</defName>
    <label>Bright Neck Fluff</label>
    <description>Carriers of this gene have a floofy mane of neck fluff. Moth approved!\n\nColored a bit brighter and more desaturated than the skin of the wearer.</description>
    <iconPath>UI/Icons/Genes/BugParts_Mothfluff</iconPath>
    <renderNodeProperties Inherit="False">
      <li Class="OopsBug.HSVPawnRenderingProps">
        <!--Messing around with the Render Node Properties...-->
        <hsvClampMin>
          <value>0.4</value>
        </hsvClampMin>
        <hsvClampMax>
          <saturation>0.25</saturation>
        </hsvClampMax>
        <hsvMultiplier>
          <saturation>0.95</saturation>
          <value>1.35</value>
        </hsvMultiplier>
        <parentTagDef>Body</parentTagDef>
        <texPath>Things/Pawn/Humanlike/BodyAttachments/Fluff/Neckfluff</texPath>
        <workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
        <nodeClass>OopsBug.PawnRenderNode_HSVTweak</nodeClass>
        <colorType>Skin</colorType>
        <drawData>
          <scaleOffsetByBodySize>true</scaleOffsetByBodySize>
          <defaultData>
            <layer>25</layer>
            <offset>(0.0, 0.0, 0.35)</offset>
          </defaultData>
          <dataEast>
            <layer>25</layer>
            <offset>(0.075, 0.0, 0.40)</offset>
          </dataEast>
          <dataWest>
            <layer>25</layer>
            <offset>(-0.075, 0.0, 0.40)</offset>
          </dataWest>
        </drawData>
        <drawSize>1</drawSize>
      </li>
    </renderNodeProperties>
  </GeneDef>

