  <GeneDef ParentName="BP_GeneeyesBase">
    <defName>Insectiod_Mantideyes</defName>
    <label>Mantis Features</label>
    <description>Carriers of this gene have large, powerful, compound insectoid eyes along with a mantis face.</description>
    <iconPath>Genes/Icons/BugParts_MantidEyesIcon</iconPath>
    <missingGeneRomanceChanceFactor>0.4</missingGeneRomanceChanceFactor>
    <capMods>
      <li>
        <capacity>Sight</capacity>
        <postFactor>1.2</postFactor>
      </li>
    </capMods>
    <forcedHeadTypes>
      <li>Mantidhead_Average1</li>
      <li>Mantidhead_Narrow1</li>
      <li>Mantidhead_Narrow2</li>
      <li>Mantidhead_Gaunt</li>
      <li>Mantidhead_HeavyJaw</li>
    </forcedHeadTypes>
    <renderNodeProperties Inherit="False">
      <li>
        <texPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/MantidEyes</texPath>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <parentTagDef>Head</parentTagDef>
        <drawData>
          <defaultData>
            <layer>54</layer>
          </defaultData>
        </drawData>
      </li>
      <li>
        <texPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/MantidEyesUpper</texPath>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <parentTagDef>Head</parentTagDef>
        <drawData>
          <defaultData>
            <layer>79</layer>
          </defaultData>
          <dataNorth>
            <layer>10</layer>
          </dataNorth>
        </drawData>
      </li>
    </renderNodeProperties>
  </GeneDef>
  <GeneDef>
    <defName>BP_Motheyes</defName>
    <label>Moth eyes</label>
    <iconPath>UI/Icons/Genes/BP_Motheyes</iconPath>
    <description>Carriers of this gene have two large eyes, that can almost see things before they happen.</description>
    <displayCategory>Bug_HeadAttachment</displayCategory>
    <displayOrderInCategory>52</displayOrderInCategory>
    <biostatMet>-1</biostatMet>
    <biostatCpx>1</biostatCpx>
    <selectionWeight>0</selectionWeight>
    <exclusionTags>
      <li>ShootingAccuracy</li>
      <li>EyeColor</li>
      <li>AG_Heads</li>
      <li>Heads</li>
      <li>Head</li>
    </exclusionTags>
    <capMods>
      <li>
        <capacity>Sight</capacity>
        <offset>0.2</offset>
      </li>
    </capMods>
    <forcedHeadTypes>
      <li>Mothhead_Average1</li>
      <li>Mothhead_Narrow1</li>
      <li>Mothhead_Gaunt</li>
      <li>Mothhead_HeavyJaw</li>
    </forcedHeadTypes>
    <renderNodeProperties Inherit="False">
      <li Class="PawnRenderNodeProperties_Eye">
        <texPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/BP_BugEyes</texPath>
        <anchorTag>RightEye</anchorTag>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <parentTagDef>Head</parentTagDef>
        <drawSize>0.2</drawSize>
        <side>Right</side>
        <drawData>
          <defaultData>
            <layer>54</layer>
            <offset>(-0.04, 0, -0.25)</offset>
          </defaultData>
        </drawData>
      </li>
      <li Class="PawnRenderNodeProperties_Eye">
        <texPath>Things/Pawn/Humanlike/HeadAttachments/BugEyes/BP_BugEyes</texPath>
        <anchorTag>LeftEye</anchorTag>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <parentTagDef>Head</parentTagDef>
        <drawSize>0.2</drawSize>
        <side>Left</side>
        <drawData>
          <defaultData>
            <layer>54</layer>
            <offset>(0.04, 0, -0.25)</offset>
            <flip>true</flip>
          </defaultData>
          <dataWest>
            <flip>false</flip>
          </dataWest>
        </drawData>
      </li>
    </renderNodeProperties>
  </GeneDef>
