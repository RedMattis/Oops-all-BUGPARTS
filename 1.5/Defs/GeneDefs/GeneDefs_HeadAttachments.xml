<Defs>
  <GeneDef Name="BP_GeneAntennaBase" Abstract="True">
    <biostatCpx>0</biostatCpx>
    <endogeneCategory>Headbone</endogeneCategory>
    <displayCategory>Bug_HeadAttachment</displayCategory>
    <displayOrderInCategory>80</displayOrderInCategory>
    <iconColor>(0.75, 0.75, 0.75)</iconColor>
    <randomChosen>true</randomChosen>
    <exclusionTags>
      <li>Antennas</li>
      <li>Antenna</li>
      <li>AG_Antennae</li>

      <!--I figure letting horns and Antenna stack could look cool.
      This is also in line with other mods, e.g. Alpha Genes, etc.-->
      <!-- <li>Headbone</li> --> 

       <!--Not sure what this tag is meant to block?
       I'd assume we want antenna and chitin to go together.-->
      <!-- <li>HeadChitin</li> --> 
    </exclusionTags>
  </GeneDef>
  <GeneDef ParentName="BP_GeneAntennaBase">
    <defName>Mantis_Antennae</defName>
    <label>mantis antennae</label>
    <description>Carriers of this gene have two long antennae.</description>
    <iconPath>UI/Icons/Genes/Gene_Antenna</iconPath>
    <renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <useSkinShader>true</useSkinShader>
        <colorType>Skin</colorType>
        <texPaths>
          <li>Things/Pawn/Humanlike/HeadAttachments/Antenna/AntennaA</li>
          <li>Things/Pawn/Humanlike/HeadAttachments/Antenna/AntennaB</li>
          <li>Things/Pawn/Humanlike/HeadAttachments/Antenna/AntennaC</li>
          <li>Things/Pawn/Humanlike/HeadAttachments/Antenna/AntennaD</li>
        </texPaths>
        <drawData>
          <defaultData>
            <layer>80</layer>
          </defaultData>
          <dataNorth>
            <layer>10</layer>
          </dataNorth>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
  </GeneDef>

