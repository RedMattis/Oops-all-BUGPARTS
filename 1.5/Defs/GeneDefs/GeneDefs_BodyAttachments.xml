  <GeneDef ParentName="GeneTailBase">
    <defName>BugParts_Abdomen_Caterpillar</defName>
    <label>Caterpillar Tail</label>
    <displayCategory>Bug_BodyAttachment</displayCategory>
    <description>Carriers of this gene grow a large Catterpillar tail.</description>
    <iconPath>Genes/Icons/Gene_Caterpillarab</iconPath>
    <displayOrderInCategory>1001</displayOrderInCategory>
    <biostatCpx>1</biostatCpx>
    <selectionWeight>0</selectionWeight>
    <renderNodeProperties Inherit="False">
      <li>
        <parentTagDef>Body</parentTagDef>
        <texPath>Things/Pawn/Humanlike/BodyAttachments/Wormies/Caterpillar</texPath>
        <drawSize>(1.25,1.25)</drawSize>
        <colorType>Skin</colorType>
        <workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
        <drawData>
          <scaleOffsetByBodySize>true</scaleOffsetByBodySize>
          <dataNorth>
            <offset>(0.0, 0.2, -0.0)</offset>
          </dataNorth>
          <dataSouth>
            <offset>(-0.0, -0.1, -0.0)</offset>
          </dataSouth>
          <dataEast>
            <offset>(-0.0, -0.1, -0.0)</offset>
          </dataEast>
        </drawData>
